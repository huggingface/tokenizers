    },
    "devDependencies": {
        "@types/jest": "^26.0.14",
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "eslint": "^7.10.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-jest": "^23.20.0",
        "eslint-plugin-jsdoc": "^30.6.1",
        "eslint-plugin-prettier": "^3.1.4",
